<!DOCTYPE html>
<html lang="pt">

<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <!-- Meta, title, CSS, favicons, etc. -->
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>GRUBSCHOOL | @ViewData["Title"]</title>

  <!-- Bootstrap -->
  <link href="~/css/bootstrap.min.css" rel="stylesheet">
  <!-- Font Awesome -->
  <link href="~/css/font-awesome.min.css" rel="stylesheet">
  <!-- NProgress -->
  <link href="~/css/nprogress.css" rel="stylesheet">
  <!-- jQuery custom content scroller -->
  <link href="~/css/jquery.mCustomScrollbar.min.css" rel="stylesheet" />
  <!-- iCheck -->
  <link href="../vendors/iCheck/skins/flat/green.css" rel="stylesheet">
  <!-- Custom Theme Style -->
  <link href="~/css/custom.min.css" rel="stylesheet">
</head>

<body class="nav-md">
  <div class="container body">
    <div class="main_container">
      <div class="col-md-3 left_col menu_fixed">
        <div class="left_col scroll-view">
          <div class="navbar nav_title" style="border: 0;">
            <a asp-area="" asp-controller="Home" asp-action="Index" class="site_title"><i><img
                  src="~/images/GRUBSCHOOL_ICO.png" alt="Grubschool logo" width="20" height="20"></i>
              <span>GRUBSCHOOL</span></a>
          </div>

          <div class="clearfix"></div>

          <!-- sidebar menu -->
          <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
            <div class="menu_section">
              <ul class="nav side-menu">
                <li><a asp-area="" asp-controller="Home" asp-action="Index"><i class="fa fa-dashboard"></i> Dashboard
                  </a></li>

                <li><a><i class="fa fa-graduation-cap"></i> Estudantes <span class="fa fa-chevron-down"></span></a>
                  <ul class="nav child_menu">
                    <li><a asp-area="" asp-controller="Estudantes" asp-action="Index">Novo Estudante</a></li>
                    <li><a asp-area="" asp-controller="Estudantes" asp-action="Index">Todos Estudantes</a></li>
                    <li><a asp-area="" asp-controller="Estudantes" asp-action="Index">Ensino Primário</a></li>
                    <li><a asp-area="" asp-controller="Estudantes" asp-action="Index">Iº Ciclo</a></li>
                    <li><a asp-area="" asp-controller="Estudantes" asp-action="Index">IIº Ciclo - Técnico</a></li>
                    <li><a asp-area="" asp-controller="Estudantes" asp-action="Index">IIº Ciclo - PUNIV</a></li>
                    <li><a asp-area="" asp-controller="Estudantes" asp-action="Index">IIº Ciclo - Saúde</a></li>
                  </ul>
                </li>

                <li><a><i class="fa fa-dollar"></i> Pagamentos <span class="fa fa-chevron-down"></span></a>
                  <ul class="nav child_menu">
                    <li><a asp-area="" asp-controller="Pagamentos" asp-action="Create">Novo Pagamento</a></li>
                    <li><a asp-area="" asp-controller="Pagamentos" asp-action="Matriculas">Matrículas</a></li>
                    <li><a asp-area="" asp-controller="Pagamentos" asp-action="Confirmacoes">Confirmações</a></li>
                    <li><a asp-area="" asp-controller="Pagamentos" asp-action="Propinas">Propinas</a></li>
                    <li><a asp-area="" asp-controller="Pagamentos" asp-action="Emolumentos">Emolumentos</a></li>
                  </ul>
                </li>

                <li><a><i class="fa fa-file-text-o"></i> Emolumentos <span class="fa fa-chevron-down"></span></a>
                  <ul class="nav child_menu">
                    <li><a asp-area="" asp-controller="Emolumentos" asp-action="Emolumentos">Novo Pedido</a></li>
                    <li><a asp-area="" asp-controller="Emolumentos" asp-action="Entregues">Entregues</a></li>
                    <li><a asp-area="" asp-controller="Emolumentos" asp-action="EmProcesso">Em Processo</a></li>
                    <li><a asp-area="" asp-controller="Emolumentos" asp-action="Pendentes">Pendentes</a></li>
                  </ul>
                </li>

                <li><a><i class="fa fa-line-chart"></i> Relatórios <span class="fa fa-chevron-down"></span></a>
                  <ul class="nav child_menu">
                    <li><a asp-area="" asp-controller="Home" asp-action="Privacy">Registros</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Privacy">Advanced Components</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Privacy">Form Validation</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Privacy">Form Wizard</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Privacy">Form Upload</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Privacy">Form Buttons</a></li>
                  </ul>
                </li>

                <li><a><i class="fa fa-calendar-o"></i> Calendário <span class="fa fa-chevron-down"></span></a>
                  <ul class="nav child_menu">
                    <li><a asp-area="" asp-controller="AnoLectivos" asp-action="Index">Ano Lectivos</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Privacy">Actividades</a></li>
                  </ul>
                </li>

                <li><a><i class="fa fa-gear"></i> Definições <span class="fa fa-chevron-down"></span></a>
                  <ul class="nav child_menu">
                    <li><a asp-area="" asp-controller="Home" asp-action="Privacy">#</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Privacy">#</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Privacy">#</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Privacy">#</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Privacy">#</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Privacy">#</a></li>
                  </ul>
                </li>

              </ul>
            </div>

          </div>
          <!-- /sidebar menu -->

        </div>
      </div>

      <!-- top navigation -->
      <div class="top_nav">
        <div class="nav_menu">
          <div class="nav toggle">
            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
          </div>

          <nav class="nav navbar-nav">
            <ul class=" navbar-right">
              <li class="nav-item dropdown open" style="padding-left: 15px;">
                <a href="javascript:;" class="user-profile dropdown-toggle" aria-haspopup="true" id="navbarDropdown"
                  data-toggle="dropdown" aria-expanded="false">
                  <img src="" alt="">John Doe
                </a>
                <div class="dropdown-menu dropdown-usermenu pull-right" aria-labelledby="navbarDropdown">
                  <a class="dropdown-item" href="javascript:;"> Profile</a>
                  <a class="dropdown-item" href="javascript:;">
                    <span class="badge bg-red pull-right">50%</span>
                    <span>Settings</span>
                  </a>
                  <a class="dropdown-item" href="javascript:;">Help</a>
                  <a class="dropdown-item" href="login.html"><i class="fa fa-sign-out pull-right"></i> Log Out</a>
                </div>
              </li>
            </ul>
          </nav>
        </div>
      </div>
      <!-- /top navigation -->

      <!-- page content -->
      <div class="right_col" role="main">
        <div class=" ">
          <div class="clearfix"></div>
          @RenderBody()
        </div>
      </div>
      <!-- /page content -->

      <!-- footer content -->
      <footer>
        <div class="pull-right">
          &copy; 2021 - GRUBSCHOOL - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
        <div class="clearfix"></div>
      </footer>
      <!-- /footer content -->
    </div>
  </div>

  <!-- jQuery -->
  <script src="~/js/jquery.min.js"></script>
  <!-- Bootstrap -->
  <script src="~/js/bootstrap.bundle.min.js"></script>
  <!-- FastClick -->
  <script src="~/js/fastclick.js"></script>
  <!-- NProgress -->
  <script src="~/js/nprogress.js"></script>
  <!-- jQuery custom content scroller -->
  <script src="~/js/jquery.mCustomScrollbar.concat.min.js"></script>

  <!-- Custom Theme Scripts -->
  <script src="js/custom.min.js"></script>

  <script src="~/js/site.js" asp-append-version="true"></script>
  @await RenderSectionAsync("Scripts", required: false)
